<p id="notice"><%= notice %></p>

<h1>Heros</h1>

<table>
  <thead>
    <tr>
      <th>Headerimg</th>
      <th>Bio</th>
      <th>Video</th>
      <th>Realname</th>
      <th>Occupation</th>
      <th>Baseoperations</th>
      <th>Affiliations</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @heros.each do |hero| %>
      <tr>
        <td><%= hero.headerImg %></td>
        <td><%= hero.bio %></td>
        <td><%= hero.video %></td>
        <td><%= hero.realName %></td>
        <td><%= hero.occupation %></td>
        <td><%= hero.baseOperations %></td>
        <td><%= hero.affiliations %></td>
        <td><%= hero.name %></td>
        <td><%= link_to 'Show', hero %></td>
        <td><%= link_to 'Edit', edit_hero_path(hero) %></td>
        <td><%= link_to 'Destroy', hero, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hero', new_hero_path %>
